<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.PerformanceSettingPage.Title">PerformanceSettingPage</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Counter">Hitung Kinerja</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Counter.Description">Apakah semua hitung kinerja akan berjalan di latar belakang atau tidak.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Counter.CounterHint">Catatan: Beberapa tema berjenis Web mungkin tidak berfungsi dengan baik, dan tidak satu pun dari pengaturan kinerja di bawah ini akan berfungsi.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Pause">Aturan Jeda</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Pause.Description">Menentukan aturan untuk memberhentikan wallpaper berdasarkan pengaturan kinerja Anda.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Communication">Jenis Komunikasi</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Communication.Description">Menentukan metode pengiriman pesan ke tema-tema tipe Web.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Cpu">Penggunaan CPU</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Cpu.Description">Hasil pengaturan penggunaan CPU akan memengaruhi apa yang terjadi pada wallpaper.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Cpu.CpuUsage">Penggunaan CPU (%):</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Gpu">Penggunaan GPU</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Gpu.Description">Apa yang terjadi pada latar belakang berdasarkan pengaturan penggunaan GPU Anda.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Gpu.GpuAdapter">GPU:</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Gpu.GpuAdapter.Empty">Daftar unit pemrosesan grafis belum dibuat</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Gpu.GpuUsage">Penggunaan GPU (%):</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Memory">Penggunaan Memori</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Memory.Description">Hasil pengaturan penggunaan memori akan memengaruhi apa yang terjadi pada wallpaper.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Memory.MemoryUsage">Penggunaan Memori (%):</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Network">Penggunaan Jaringan</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Network.Description">Hasil pengaturan penggunaan jaringan akan memengaruhi apa yang terjadi pada wallpaper.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Network.NetworkAdapter">Adapter Jaringan:</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Network.NetworkAdapter.Empty">Daftar adapter jaringan belum dibuat</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Network.NetworkUpload">Ukuran Unggahan:</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Network.NetworkDownload">Ukuran Unduhan:</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Network.NetworkPing">Nilai Ping (MS):</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Battery">Daya Baterai</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Battery.Description">Apa yang terjadi pada wallpaper saat bekerja dengan daya baterai laptop.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Battery.BatteryUsage">Status Baterai (%):</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.BatterySaver">Penghemat Baterai</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.BatterySaver.Description">Apa yang terjadi pada wallpaper saat laptop berada dalam mode penghematan baterai.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Virtual">Virtualisasi</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Virtual.Description">Apa yang terjadi pada wallpaper saat aplikasi virtualisasi berjalan.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Remote">Desktop Remote</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Remote.Description">Apa yang terjadi pada wallpaper saat aplikasi desktop remote berjalan.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.FullScreen">Mode Layar Penuh</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.FullScreen.Description">Apa yang terjadi pada wallpaper saat aplikasi apa pun berjalan dalam mode layar penuh.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Focus">Mode Fokus</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Focus.Description">Apa yang terjadi pada wallpaper saat ada aplikasi lain yang sedang difokuskan.</system:String>
</ResourceDictionary>