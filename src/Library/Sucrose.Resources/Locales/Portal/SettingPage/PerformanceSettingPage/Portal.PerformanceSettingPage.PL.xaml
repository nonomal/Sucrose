<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.PerformanceSettingPage.Title">PerformanceSettingPage</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Counter">Liczniki Wydajności</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Counter.Description">Czy wszystkie liczniki wydajności powinny działać w tle.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Counter.CounterHint">Uwaga: Niektóre tapety o tematyce Web mogą nie działać poprawnie, i żadne z poniższych ustawień wydajności nie będzie działać.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Pause">Reguła Pauzy</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Pause.Description">Określa regułę zatrzymywania tapety na podstawie ustawień wydajności.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Communication">Typ Komunikacji</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Communication.Description">Określa metodę wysyłania wiadomości do motywów typu Web.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Cpu">Użycie CPU</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Cpu.Description">Co dzieje się z tapetą na podstawie ustawień użycia CPU.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Cpu.CpuUsage">Użycie CPU (%):</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Gpu">Użycie GPU</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Gpu.Description">Co się dzieje z tapetą w zależności od ustawień użycia GPU.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Gpu.GpuAdapter">GPU:</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Gpu.GpuAdapter.Empty">Lista jednostek przetwarzania grafiki nie została jeszcze utworzona</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Gpu.GpuUsage">Użycie GPU (%):</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Memory">Użycie Pamięci</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Memory.Description">Co dzieje się z tapetą na podstawie ustawień użycia pamięci.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Memory.MemoryUsage">Użycie Pamięci (%):</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Network">Użycie Sieci</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Network.Description">Co dzieje się z tapetą na podstawie ustawień użycia sieci.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Network.NetworkAdapter">Karta Sieciowa:</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Network.NetworkAdapter.Empty">Nie utworzono jeszcze listy kart sieciowych</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Network.NetworkUpload">Wielkość Wysyłania:</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Network.NetworkDownload">Wielkość Pobierania:</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Network.NetworkPing">Wartość Ping (ms):</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Lock">Blokada Sesji</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Lock.Description">Co się stanie z tapetą, gdy sesja jest zablokowana.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Sleep">Tryb Uśpienia</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Sleep.Description">Co się stanie z tapetą, gdy system jest w trybie uśpienia.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Console">Zmiana Użytkownika</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Console.Description">Co się stanie z tapetą, gdy zmienia się użytkownik.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Session">Wylogowanie</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Session.Description">Co się stanie z tapetą, gdy sesja jest zamykana.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.ScreenSaver">Wygaszacz Ekranu</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.ScreenSaver.Description">Co się stanie z tapetą, gdy włączany jest wygaszacz ekranu.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Battery">Zasilanie Baterii</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Battery.Description">Co dzieje się z tapetą, gdy działa na zasilaniu baterii laptopa.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Battery.BatteryUsage">Stan Baterii (%):</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.BatterySaver">Tryb Oszczędzania Baterii</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.BatterySaver.Description">Co dzieje się z tapetą w trybie oszczędzania baterii laptopa.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Virtual">Wirtualizacja</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Virtual.Description">Co dzieje się z tapetą podczas działania aplikacji wirtualizacji.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Remote">Pulpit Zdalny</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Remote.Description">Co dzieje się z tapetą podczas działania aplikacji pulpitu zdalnego.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.FullScreen">Tryb Pełnoekranowy</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.FullScreen.Description">Co dzieje się z tapetą, gdy dowolna aplikacja działa w trybie pełnoekranowym.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Focus">Tryb Skupienia</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Focus.Description">Co dzieje się z tapetą, gdy inna aplikacja ma pierwszy plan.</system:String>
</ResourceDictionary>