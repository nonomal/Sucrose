<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.PerformanceSettingPage.Title">PerformanceSettingPage</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Counter">Pengiraan Prestasi</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Counter.Description">Adakah semua pengiraan prestasi berfungsi di latar belakang.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Counter.CounterHint">Nota: Sesetengah tema jenis Web mungkin tidak berfungsi dengan baik, dan tiada pengaturan prestasi di bawah akan berfungsi.</system:String>

    <system:String x:Key="Portal.PerformanceSettingPage.Pause">Peraturan Jeda</system:String>
    <system:String x:Key="Portal.PerformanceSettingPage.Pause.Description">Menentukan peraturan untuk menghentikan latar belakang berdasarkan tetapan prestasi anda.</system:String>

