<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.WallpaperSettingPage.Title">WallpaperSettingPage</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.InputMode">Interaksjonsmetode</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.InputMode.Description">Bestemmer metodene der interaksjon kan forekomme med støttede bakgrunnsbilder.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.InputMode.InputModule">Interaksjonsmodul:</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.InputMode.InputDesktop">Behandle interaksjon kun når skrivebordet er i fokus</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.InputMode.DesktopIcon">Skjul for å unngå utilsiktet valg av skrivebordsikoner</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.ScreenLayout">Skjermoppsett</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.ScreenLayout.Description">Endrer plasseringen av bakgrunnsbilder på skjermen din.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.StretchMode">Strekningmodus</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.StretchMode.Description">Endrer skaleringen av støttede bakgrunnsbilder.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.LoopMode">Løkke-Modus</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.LoopMode.Description">Endrer løkke-modus for støttede bakgrunnsbilder.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.ShuffleMode">Blandingsmodus</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.ShuffleMode.Description">Endrer blandingsmodus for støttede bakgrunnsbilder.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.BackgroundImage">Bakgrunnsbilde</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.BackgroundImage.Description">Endrer levende bakgrunnsbilde til et skrivebordsbakgrunnsbilde.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.HardwareAcceleration">Maskinvareakselerasjon</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.HardwareAcceleration.Description">Endrer maskinvareakselerasjonsstatus for støttede bakgrunnsbilder.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.GifPlayer">Gif-Spiller</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.GifPlayer.Description">Velg en plugin som vil spille av temaer av typen Gif i biblioteket ditt.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.VideoPlayer">Video-Spiller</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.VideoPlayer.Description">Velg en plugin som vil spille av temaer av typen Video i biblioteket ditt.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.UrlPlayer">Url-Motor</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.UrlPlayer.Description">Velg en motor som vil kjøre temaer av typen Url i biblioteket ditt.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.WebPlayer">Web-Motor</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.WebPlayer.Description">Velg en motor som vil kjøre temaer av typen Web i biblioteket ditt.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.YouTubePlayer">YouTube-Motor</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.YouTubePlayer.Description">Velg en motor som vil kjøre temaer av typen YouTube i biblioteket ditt.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.ApplicationPlayer">Application-Motor</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.ApplicationPlayer.Description">Velg en motor som vil kjøre temaer av typen Application i biblioteket ditt.</system:String>
</ResourceDictionary>