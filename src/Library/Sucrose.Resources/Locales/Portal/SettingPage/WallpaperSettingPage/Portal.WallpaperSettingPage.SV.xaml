<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.WallpaperSettingPage.Title">WallpaperSettingPage</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.InputMode">Interaktionsmetod</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.InputMode.Description">Bestämmer metoderna genom vilka interaktion kan ske med stödda bakgrundsbilder.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.InputMode.InputModule">Interaktionsmodul:</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.InputMode.InputDesktop">Bearbeta interaktion endast när skrivbordet är i fokus</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.InputMode.DesktopIcon">Göm för att undvika oavsiktlig val av skrivbordsikoner</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.ScreenLayout">Skärm Layout</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.ScreenLayout.Description">Ändrar hur dina tapeter är placerade på skärmen.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.StretchMode">Storlägesmetod</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.StretchMode.Description">Ändrar skalningsmetoden för stödda tapeter.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.StayAwake">Håll dig Vaken</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.StayAwake.Description">Ändrar vakenstatus för stödda tapeter.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.LoopMode">Loopläge</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.LoopMode.Description">Ändrar upprepningssättet för stödda tapeter.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.ShuffleMode">Blandningsläge</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.ShuffleMode.Description">Ändrar blandningsläge för stödda tapeter.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.BackgroundImage">Bakgrundsbild</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.BackgroundImage.Description">Ändrar det levande tapeten till en skrivbordsbakgrundsbild.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.HardwareAcceleration">Hårdvaruacceleration</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.HardwareAcceleration.Description">Ändrar hårdvaruaccelerationsstatus för stödda bakgrundsbilder.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.GifPlayer">Gif-Spelare</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.GifPlayer.Description">Välj en plugin som kommer att spela Gif-teman i din samling.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.VideoPlayer">Videospelare</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.VideoPlayer.Description">Välj en plugin som kommer att spela Video-teman i din samling.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.UrlPlayer">Url Motor</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.UrlPlayer.Description">Välj en motor som kommer att spela Url-teman i din samling.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.WebPlayer">Webbmotor</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.WebPlayer.Description">Välj en motor som kommer att spela Web-teman i din samling.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.YouTubePlayer">YouTube Motor</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.YouTubePlayer.Description">Välj en motor som kommer att spela YouTube-teman i din samling.</system:String>

    <system:String x:Key="Portal.WallpaperSettingPage.ApplicationPlayer">Application Motor</system:String>
    <system:String x:Key="Portal.WallpaperSettingPage.ApplicationPlayer.Description">Välj en motor som kommer att spela Application-teman i din samling.</system:String>
</ResourceDictionary>